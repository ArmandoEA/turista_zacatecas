package clases;

import java.util.ArrayList;

public class Propiedad {
	private int id, precio, hipoteca, precio_casa, renta, num_casas, dueño;
	private ArrayList<Integer> renta_casas;
	private String nombre, imagen;
	private boolean hipotecada, se_puede_construir;
	
	public Propiedad(int id, String nombre, int renta, int precio_casa, ArrayList<Integer> renta_casas){
		this.id = id;
		this.nombre = nombre;
		this.renta = renta;
		this.precio_casa =  precio_casa;
	}
}
